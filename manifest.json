{
  "manifest_version": 2,
  "name": "Test",
  "description": "Test Extension",
  "version": "1.0",

  "icons": {
    "48": "icon.png"
   },

  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [{
    "matches": ["https://www.amazon.com/*"],
    "js": ["jquery.min.js", "jquery.popupoverlay.js", "why.js"],
    "run_at": "document_end"
  }],
  "browser_action": {
    "default_title": "This is a test",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
